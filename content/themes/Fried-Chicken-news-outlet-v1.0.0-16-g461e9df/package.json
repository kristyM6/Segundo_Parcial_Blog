{
  "name": "news-outlet",
  "description": "A new outlet style theme for Ghost",
  "demo": "",
  "version": "1.0.0",
  "engines": {
    "ghost": ">=2.0.0",
    "ghost-api": "v2"
  },
  "main": "index.js",
  "scripts": {
    "dev": "gulp",
    "zip": "gulp zip",
    "test": "gscan ."
  },
  "repository": {
    "type": "git",
    "url": "MIT"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Newspaper",
      "Broadsheet",
      "Magazine"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme",
    "blog",
    "magazine",
    "newspaper"
  ],
  "author": {
    "name": "Fried-Chicken",
    "email": "Fried-Chicken@users.noreply.github.com",
    "url": "https://github.com/Fried-Chicken"
  },
  "bugs": "https://github.com/Fried-Chicken/ghost-theme-news-outlet/issues",
  "contributors": "https://github.com/Fried-Chicken/ghost-theme-news-outlet/graphs/contributors",
  "license": "MIT",
  "screenshots": {
    "desktop": "assets/screenshot-desktop.jpg",
    "mobile": "assets/screenshot-mobile.jpg"
  },
  "config": {
    "posts_per_page": 25,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "cssnano": "3.7.1",
    "gscan": "^2.0.0",
    "gulp": "3.9.1",
    "gulp-livereload": "3.8.1",
    "gulp-postcss": "6.1.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.8",
    "gulp-zip": "4.0.0",
    "postcss-color-function": "2.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-easy-import": "1.0.1",
    "gulp-filter": "5.1.0",
    "gulp-uglify": "3.0.1"
  }
}
